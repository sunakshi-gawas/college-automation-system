<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Payment_home_page">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="21dp"
        android:layout_marginTop="184dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.459"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/linearLayout36"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginTop="104dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/white_500"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingStart="20dp"
            android:paddingTop="10dp"
            android:text="PAYMENT SECTION"
            android:textColor="@color/black"
            android:textSize="16sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="207dp" />


        <LinearLayout
            android:id="@+id/linearLayout8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="26dp"
            tools:layout_editor_absoluteY="298dp">

            <Button
                android:id="@+id/btn_clg_jump"
                android:layout_width="95dp"
                android:layout_height="85dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/ic_baseline_account_balance_24"
                android:padding="20dp"
                tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


            <Button
                android:id="@+id/btn_gohostel"
                android:layout_width="95dp"
                android:layout_height="85dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/hostel"
                android:padding="20dp"
                tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


            <Button
                android:id="@+id/btn_goexamfee"
                android:layout_width="95dp"
                android:layout_height="85dp"
                android:layout_marginLeft="30dp"
                android:background="@drawable/exam"
                tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="391dp">

            <TextView
                android:id="@+id/textView27"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="College fee  "
                android:textAlignment="center"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Hostel fee "
                android:textAlignment="center"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Exam fee "
                android:textAlignment="center"
                android:textColor="@color/black"
                tools:ignore="MissingConstraints" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout34"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="25dp"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="10dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/white_500"
        android:orientation="vertical"

        app:layout_constraintTop_toBottomOf="@+id/linearLayout36">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="12dp"
            tools:layout_editor_absoluteY="445dp">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="20dp"
                android:text="PAYMENT HISTORY"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="MissingConstraints" />

            <LinearLayout
                android:id="@+id/linearLayout8asdsadadasd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:orientation="horizontal"
                app:layout_constraintBottom_toTopOf="@+id/linearLayout14"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView">

                <Button
                    android:id="@+id/paymenthistory"
                    android:layout_width="95dp"
                    android:layout_height="80dp"
                    android:layout_marginStart="50dp"
                    android:background="@drawable/ic_baseline_content_paste_search_24"
                    android:padding="20dp"

                    tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />


                <Button
                    android:id="@+id/button52"
                    android:layout_width="95dp"
                    android:layout_height="80dp"
                    android:layout_marginStart="80dp"
                    android:background="@drawable/supportpay" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayout14adsdadsadadadasad"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView27adasdadad"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingStart="30dp"
                    android:text="payment history"
                    android:textColor="@color/black"
                    tools:ignore="MissingConstraints" />

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="325dp"
        android:layout_height="247dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="41dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout36"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.airbnb.lottie.LottieAnimationView
            android:id="@+id/ani_social"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:lottie_autoPlay="true"
            app:lottie_loop="true"
            app:lottie_rawRes="@raw/mobilepayment" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>