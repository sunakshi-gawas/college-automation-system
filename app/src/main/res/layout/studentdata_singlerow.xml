<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginEnd="10dp"
    android:layout_marginStart="10dp"
    android:background="@color/white_200"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    app:cardCornerRadius="5dp"
    android:elevation="4dp"
    app:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="376dp"
        android:layout_height="324dp"

        android:textAlignment="center"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/t1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Name"
                android:textAlignment="center"
                android:textColor="#000"
                android:textSize="25dp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView88"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:layout_weight="1"
                android:background="@color/white_500"
                android:text=" College Details"
                android:textColor="@color/black" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView130"
                android:layout_width="197dp"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Admission Type " />

            <TextView
                android:id="@+id/u3"
                android:layout_width="223dp"
                android:layout_height="match_parent"
                android:layout_below="@+id/u1"
                android:layout_alignParentStart="true"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView131"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Category" />

            <TextView
                android:id="@+id/u4"
                android:layout_width="222dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u2"
                android:layout_alignParentEnd="true"
                android:layout_toEndOf="@+id/u3"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView135"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Academic Year" />

            <TextView
                android:id="@+id/u5"
                android:layout_width="220dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u3"
                android:layout_alignParentStart="true"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView137"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Class" />

            <TextView
                android:id="@+id/u6"
                android:layout_width="220dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u4"
                android:layout_alignParentEnd="true"
                android:layout_toEndOf="@+id/u5"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView138"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" UniqueID" />

            <TextView
                android:id="@+id/u7"
                android:layout_width="219dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u5"
                android:layout_alignParentStart="true"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView145"
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:background="@color/white_500"
                android:text=" Personal Details"
                android:textColor="@color/black" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView146"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Email" />

            <TextView
                android:id="@+id/u1"
                android:layout_width="216dp"
                android:layout_height="25dp"
                android:layout_below="@+id/t1"
                android:layout_alignParentStart="true"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView147"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Gender" />

            <TextView
                android:id="@+id/u9"
                android:layout_width="217dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u7"
                android:layout_alignParentStart="true"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView148"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Date of Birth" />

            <TextView
                android:id="@+id/u8"
                android:layout_width="217dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u6"
                android:layout_alignParentEnd="true"
                android:layout_toEndOf="@+id/u7"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView149"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Mobile Number" />

            <TextView
                android:id="@+id/u2"
                android:layout_width="216dp"
                android:layout_height="25dp"
                android:layout_below="@+id/t1"
                android:layout_alignParentEnd="true"
                android:layout_toEndOf="@+id/u1"
                android:text=" TextView" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView150"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:layout_weight="1"
                android:text=" Address" />

            <TextView
                android:id="@+id/u10"
                android:layout_width="217dp"
                android:layout_height="25dp"
                android:layout_below="@+id/u8"
                android:layout_alignParentEnd="true"
                android:layout_toEndOf="@+id/u9"
                android:text=" TextView" />
        </LinearLayout>


    </LinearLayout>

</androidx.cardview.widget.CardView>